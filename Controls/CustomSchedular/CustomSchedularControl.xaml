<?xml version="1.0" encoding="utf-8" ?>
<ContentView
    x:Class="CustomSchedularControl.Controls.CustomSchedular.CustomSchedularControl"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">
    <ScrollView>
        <Grid
            x:Name="SchedularGrid"
            ColumnDefinitions="Auto,1,*"
            ColumnSpacing="5">
            <BoxView
                x:Name="BoxViewVerticalLine"
                Grid.RowSpan="48"
                Grid.Column="1" />
        </Grid>
    </ScrollView>
</ContentView>
